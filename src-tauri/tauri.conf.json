{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "dormitory_signage",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "dormitory_signage",
        "width": 1920,
        "height": 1080,
        "fullscreen": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "jp.ac.yonago-k",
      "icon": [
        "icons/rounded_nityc_aniv_logo.png",
        "icons/icon.ico"
      ]
    }
  }
}
